<?xml version="1.0" encoding="utf-8"?>
<!--suppress ALL -->
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:background="@drawable/background"
xmlns:tools="http://schemas.android.com/tools"
xmlns:app="http://schemas.android.com/apk/res-auto"
tools:context=".cadastro">

<Space
    android:layout_height="wrap_content"
    android:layout_width="wrap_content"
    tools:ignore="MissingConstraints"/>

<ImageView
    android:layout_height="61dp"
    android:layout_width="67dp"
    tools:ignore="MissingConstraints"
    app:srcCompat="@drawable/logo_redonda_"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    android:layout_marginTop="20dp"
    android:layout_marginStart="32dp"
    android:id="@+id/logo"/>

<TextView
    android:background="#FFFFFF"
    android:layout_height="wrap_content"
    android:layout_width="wrap_content"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    android:layout_marginTop="10dp"
    android:layout_marginStart="10dp"
    app:layout_constraintVertical_bias="0.048"
    app:layout_constraintHorizontal_bias="0.434"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintBottom_toBottomOf="parent"
    android:visibility="visible"
    android:textStyle="bold"
    android:textSize="20sp"
    android:textColor="#000000"
    android:text="  Cadastro Aluno"
    android:layout_marginBottom="10dp"
    android:layout_marginEnd="10dp"/>

<TextView
    android:layout_height="wrap_content"
    android:layout_width="wrap_content"
    tools:ignore="MissingConstraints"
    app:layout_constraintStart_toStartOf="parent"
    android:layout_marginTop="10dp"
    android:id="@+id/titulo_nomecompleto"
    android:labelFor="@+id/nome_completo"
    app:layout_constraintHorizontal_bias="0.208"
    app:layout_constraintEnd_toEndOf="parent"
    android:textStyle="bold"
    android:textSize="20sp"
    android:textColor="#000000"
    android:text="Nome Completo"
    app:layout_constraintTop_toBottomOf="@+id/logo"/>

<EditText
    android:background="#FFFFFF"
    android:layout_height="48dp"
    android:layout_width="300dp"
    tools:ignore="MissingConstraints"
    app:layout_constraintStart_toStartOf="parent"
    android:layout_marginTop="3dp"
    android:id="@+id/nome_completo"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/titulo_nomecompleto"
    android:inputType="textPersonName"
    android:ems="10"/>

<TextView
    android:layout_height="wrap_content"
    android:layout_width="wrap_content"
    tools:ignore="MissingConstraints"
    app:layout_constraintStart_toStartOf="parent"
    android:layout_marginTop="10dp"
    android:layout_marginStart="52dp"
    android:id="@+id/titulo_matricula"
    android:labelFor="@+id/cod_matricula"
    android:textStyle="bold"
    android:textSize="20sp"
    android:textColor="#000000"
    android:text="Matricula"
    app:layout_constraintTop_toBottomOf="@+id/nome_completo"/>

<EditText
    android:background="#FBFBFB"
    android:layout_height="48dp"
    android:layout_width="145dp"
    tools:ignore="MissingConstraints"
    app:layout_constraintStart_toStartOf="parent"
    android:layout_marginTop="3dp"
    android:id="@+id/cod_matricula"
    app:layout_constraintHorizontal_bias="0.199"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/titulo_matricula"
    android:inputType="number"
    android:ems="10"
    android:autoText="false"/>

<TextView
    android:layout_height="wrap_content"
    android:layout_width="wrap_content"
    tools:ignore="MissingConstraints"
    android:layout_marginTop="10dp"
    android:layout_marginStart="76dp"
    android:id="@+id/titulo_rg"
    android:labelFor="@+id/rg"
    android:textStyle="bold"
    android:textSize="20sp"
    android:textColor="#000000"
    android:text="RG"
    app:layout_constraintTop_toBottomOf="@+id/nome_completo"
    app:layout_constraintStart_toEndOf="@+id/titulo_matricula"/>

<EditText
    android:background="#FFFFFF"
    android:layout_height="48dp"
    android:layout_width="145dp"
    tools:ignore="MissingConstraints"
    android:layout_marginTop="3dp"
    android:id="@+id/rg"
    app:layout_constraintHorizontal_bias="0.205"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/titulo_rg"
    android:inputType="number"
    android:ems="10"
    app:layout_constraintStart_toEndOf="@+id/cod_matricula"/>

<TextView
    android:layout_height="wrap_content"
    android:layout_width="wrap_content"
    tools:ignore="MissingConstraints"
    app:layout_constraintStart_toStartOf="parent"
    android:layout_marginTop="10dp"
    android:layout_marginStart="52dp"
    android:id="@+id/titulo_telefone"
    android:labelFor="@+id/numero_telefone"
    android:textStyle="bold"
    android:textSize="20sp"
    android:textColor="#000000"
    android:text="Telefone"
    app:layout_constraintTop_toBottomOf="@+id/cod_matricula"/>

<EditText
    android:background="#FFFFFF"
    android:layout_height="48dp"
    android:layout_width="145dp"
    tools:ignore="MissingConstraints"
    app:layout_constraintStart_toStartOf="parent"
    android:layout_marginTop="3dp"
    android:id="@+id/numero_telefone"
    app:layout_constraintHorizontal_bias="0.808"
    app:layout_constraintTop_toBottomOf="@+id/titulo_telefone"
    android:inputType="number"
    android:ems="10"
    app:layout_constraintEnd_toStartOf="@+id/editTextNumber5"/>

<TextView
    android:layout_height="wrap_content"
    android:layout_width="wrap_content"
    tools:ignore="MissingConstraints"
    android:layout_marginTop="10dp"
    android:layout_marginStart="80dp"
    android:id="@+id/titulo_cep"
    android:labelFor="@+id/editTextNumber5"
    android:textStyle="bold"
    android:textSize="20sp"
    android:textColor="#000000"
    android:text="CEP"
    app:layout_constraintTop_toBottomOf="@+id/rg"
    app:layout_constraintStart_toEndOf="@+id/titulo_telefone"/>

<EditText
    android:background="#FFFFFF"
    android:layout_height="48dp"
    android:layout_width="145dp"
    tools:ignore="MissingConstraints"
    android:layout_marginTop="5dp"
    android:id="@+id/editTextNumber5"
    app:layout_constraintEnd_toEndOf="parent"
    android:layout_marginEnd="52dp"
    app:layout_constraintTop_toBottomOf="@+id/titulo_cep"
    android:inputType="number"
    android:ems="10"/>

<TextView
    android:layout_height="wrap_content"
    android:layout_width="wrap_content"
    tools:ignore="MissingConstraints"
    app:layout_constraintStart_toStartOf="parent"
    android:layout_marginTop="10dp"
    android:layout_marginStart="56dp"
    android:id="@+id/titulo_logradouro"
    android:labelFor="@+id/logradouro"
    android:textStyle="bold"
    android:textSize="20sp"
    android:textColor="#000000"
    android:text="Logradouro"
    app:layout_constraintTop_toBottomOf="@+id/numero_telefone"/>

<EditText
    android:background="#FFFFFF"
    android:layout_height="48dp"
    android:layout_width="145dp"
    tools:ignore="MissingConstraints"
    app:layout_constraintStart_toStartOf="parent"
    android:layout_marginTop="3dp"
    android:id="@+id/logradouro"
    app:layout_constraintHorizontal_bias="0.859"
    app:layout_constraintTop_toBottomOf="@+id/titulo_logradouro"
    android:inputType="textPersonName"
    android:ems="10"
    app:layout_constraintEnd_toStartOf="@+id/complemento"/>

<TextView
    android:layout_height="wrap_content"
    android:layout_width="wrap_content"
    tools:ignore="MissingConstraints"
    android:layout_marginTop="10dp"
    android:layout_marginStart="52dp"
    android:id="@+id/titulo_complemento"
    android:labelFor="@+id/complemento"
    android:textStyle="bold"
    android:textSize="20sp"
    android:textColor="#000000"
    android:text="Complemento"
    app:layout_constraintTop_toBottomOf="@+id/editTextNumber5"
    app:layout_constraintStart_toEndOf="@+id/titulo_logradouro"/>

<EditText
    android:background="#FFFFFF"
    android:layout_height="48dp"
    android:layout_width="145dp"
    tools:ignore="MissingConstraints"
    android:layout_marginTop="3dp"
    android:id="@+id/complemento"
    app:layout_constraintEnd_toEndOf="parent"
    android:layout_marginEnd="52dp"
    app:layout_constraintTop_toBottomOf="@+id/titulo_complemento"
    android:inputType="textPersonName"
    android:ems="10"/>

<TextView
    android:layout_height="wrap_content"
    android:layout_width="wrap_content"
    tools:ignore="MissingConstraints"
    app:layout_constraintStart_toStartOf="parent"
    android:layout_marginTop="10dp"
    android:layout_marginStart="52dp"
    android:id="@+id/titulo_instituicao"
    android:labelFor="@+id/instituicao"
    android:textStyle="bold"
    android:textSize="20sp"
    android:textColor="#000000"
    android:text="Instituição"
    app:layout_constraintTop_toBottomOf="@+id/logradouro"/>

<EditText
    android:background="#FFFFFF"
    android:layout_height="48dp"
    android:layout_width="300dp"
    tools:ignore="MissingConstraints"
    app:layout_constraintStart_toStartOf="parent"
    android:layout_marginTop="3dp"
    android:id="@+id/instituicao"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/titulo_instituicao"
    android:inputType="textPersonName"
    android:ems="10"/>

<TextView
    android:layout_height="wrap_content"
    android:layout_width="wrap_content"
    tools:ignore="MissingConstraints"
    app:layout_constraintStart_toStartOf="parent"
    android:layout_marginTop="10dp"
    android:layout_marginStart="52dp"
    android:id="@+id/titulo_senha"
    android:labelFor="@+id/senha"
    android:textStyle="bold"
    android:textSize="20sp"
    android:textColor="#000000"
    android:text="Senha"
    app:layout_constraintTop_toBottomOf="@+id/instituicao"/>

<EditText
    android:background="#FFFFFF"
    android:layout_height="48dp"
    android:layout_width="145dp"
    tools:ignore="MissingConstraints"
    app:layout_constraintStart_toStartOf="parent"
    android:layout_marginTop="3dp"
    android:id="@+id/senha"
    app:layout_constraintHorizontal_bias="0.739"
    app:layout_constraintTop_toBottomOf="@+id/titulo_senha"
    android:inputType="textPersonName"
    android:ems="10"
    app:layout_constraintEnd_toStartOf="@+id/confirmar_senha"/>

<TextView
    android:id="@+id/titulo_confirmar_senha"
    android:layout_width="160dp"
    android:layout_height="27dp"
    android:layout_marginTop="10dp"
    android:labelFor="@+id/confirmar_senha"
    android:text="Confirmar Senha"
    android:textColor="#000000"
    android:textSize="20sp"
    android:textStyle="bold"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.753"
    app:layout_constraintStart_toEndOf="@+id/titulo_senha"
    app:layout_constraintTop_toBottomOf="@+id/instituicao"
    tools:ignore="MissingConstraints,TextSizeCheck" />

<EditText
    android:background="#FFFFFF"
    android:layout_height="48dp"
    android:layout_width="145dp"
    tools:ignore="MissingConstraints"
    android:layout_marginTop="3dp"
    android:id="@+id/confirmar_senha"
    app:layout_constraintEnd_toEndOf="parent"
    android:layout_marginEnd="52dp"
    app:layout_constraintTop_toBottomOf="@+id/titulo_confirmar_senha"
    android:inputType="textPersonName"
    android:ems="10"/>

<TextView
    android:id="@+id/titulo_codigorota"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="52dp"
    android:layout_marginTop="10dp"
    android:labelFor="@+id/codigo_rota"
    android:text="Código da Rota"
    android:textColor="#000000"
    android:textSize="20sp"
    android:textStyle="bold"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/senha"
    tools:ignore="MissingConstraints,TextContrastCheck" />

<EditText
    android:background="#FFFFFF"
    android:layout_height="48dp"
    android:layout_width="145dp"
    tools:ignore="MissingConstraints"
    app:layout_constraintStart_toStartOf="parent"
    android:layout_marginTop="3dp"
    android:layout_marginStart="52dp"
    android:id="@+id/codigo_rota"
    app:layout_constraintVertical_bias="0.0"
    app:layout_constraintBottom_toBottomOf="parent"
    android:textColor="#FFFFFF"
    app:layout_constraintTop_toBottomOf="@+id/titulo_codigorota"
    android:inputType="number"
    android:ems="10"/>

<Button
    android:background="#D637D4"
    android:layout_height="wrap_content"
    android:layout_width="wrap_content"
    tools:ignore="MissingConstraints"
    android:id="@+id/button"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintBottom_toBottomOf="parent"
    android:textStyle="bold"
    android:text="ENVIAR"
    app:layout_constraintTop_toBottomOf="@+id/confirmar_senha"
    app:layout_constraintStart_toEndOf="@+id/codigo_rota"
    app:backgroundTint="#D637D4"/>

</androidx.constraintlayout.widget.ConstraintLayout>